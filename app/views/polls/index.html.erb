<h1>Listing polls</h1>

<table>
  <thead>
  <tr>
    <th>Owner</th>
    <th>Question</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @polls.each do |poll| %>
      <tr>
        <td><%= poll.owner %></td>
        <td><%= poll.question %></td>
        <td><%= poll.name%></td>
        <td><%= link_to 'Show', poll %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poll', new_poll_path %>
